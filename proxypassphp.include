    SetEnvIfNoCase ^Authorization$ "(.+)" HTTP_AUTHORIZATION=$1
	
    <FilesMatch \.php$>
        SetHandler "proxy:unix:/var/run/php-fpm/[% vhost.user %].sock|fcgi://localhost[% vhost.documentroot %]/"
    </FilesMatch>

    <Directory [% vhost.documentroot %]>
        AllowOverride All
    </Directory>
